  "bugs": {
    "url": "https://github.com/braxtondiggs/Articuno/issues"
  },
  "homepage": "https://github.com/braxtondiggs/Articuno#readme",
  "dependencies": {
    "@types/async": "^2.0.49",
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/lodash": "^4.14.108",
    "@types/mongoose": "^5.0.12",
    "@types/request": "^2.47.0",
    "async": "^2.6.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.10",
    "mongoose": "^5.0.17",
    "request": "^2.85.0",
    "streaming-s3": "^0.4.4",
    "typescript": "^2.8.3",
    "youtube-api": "^2.0.9",
    "ytdl-core": "^0.20.2"
  },
  "devDependencies": {
    "husky": "^0.15.0-rc.13",
    "nodemon": "^1.17.4",
    "tslint": "^5.10.0"
  },
